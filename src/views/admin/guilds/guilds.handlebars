<main>
    <div class="p-5 mb-4 bg-dark rounded-3">
        <button onclick="history.back()" class="btn btn-primary">Voltar</button>
        <div class="container-fluid py-5">
            <h2>Lista de Equipes</h2>
            <hr>
            <a href="/rota_guilds/guilds/add">
                <button class="btn btn-success">Novo Equipe</button></a>
            {{#each guilds}}
            <div class="card mt-4">
                <div class="card-body">
                    <h4><b>ID:</b>{{_id}}</h4>
                    <small><b>Nome:</b> {{nome}} </small><br>
                    <a href="/rota_guilds/editar_guilds/{{_id}}">
                        <button class="btn btn-success mt-3">
                            Editar Equipe
                        </button>
                    </a>
                    <!--botÃ£o para deletar guilds -->
                    <a href="/rota_guilds/deletar_guilds/{{_id}}">
                        <button class="btn btn-danger mt-3">
                            Deletar Equipe
                        </button>
                    </a>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</main>